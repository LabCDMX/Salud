            //TECHO

            <TextView
                android:id="@+id/tvTecho"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/tvPiso"
                android:layout_marginTop="20dp"
                android:textSize="20sp"
                android:text="Techo:"
