                android:layout_marginLeft="80dp"/>

            <ImageButton
                android:id="@+id/btn_edit12"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginLeft="600dp"
                android:background="#fff"
                android:layout_below="@+id/btn_edit11"
                app:srcCompat="@drawable/pencil_pink"/>




            //ALIMENTACIÓN
            <TextView
                android:id="@+id/tvAlimentos"
                android:layout_marginTop="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/textViewTecho"
                android:textStyle="bold"
                android:textColor="@android:color/black"
                android:textSize="30sp"
                android:text="Alimentación"/>

            <View
                android:id="@+id/divider4"
                android:layout_below="@id/tvAlimentos"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginBottom="3dp"
                android:layout_marginTop="3dp"
                android:background="#E91A8D" />

            <TextView
                android:id="@+id/tvCantidades"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/divider4"
                android:layout_marginTop="20dp"
                android:textSize="20sp"
                android:text="Cantidad de comidas consumidas al día:"
                android:textStyle="bold"
                android:layout_marginLeft="10dp"
