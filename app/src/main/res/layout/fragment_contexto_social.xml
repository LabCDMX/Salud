            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">

            <TextView
                android:id="@+id/textViewPersonales"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:textColor="@android:color/black"
                android:layout_marginTop="10dp"
                android:textSize="30sp"
                android:text="Antecedentes no Patológicos"/>

            <View
                android:id="@+id/divider"
                android:layout_below="@id/textViewPersonales"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginBottom="3dp"
                android:layout_marginTop="3dp"
                android:background="#E91A8D" />

//ESCOLARIDAD
            <TextView
                android:id="@+id/tvNombre"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:textSize="20sp"
                android:layout_below="@+id/divider"
                android:text="Escolaridad Máxima:"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:textStyle="bold"
                android:textColor="@android:color/black" />

            <EditText
                android:id="@+id/textViewNombre"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:textSize="20sp"
                android:text="Nombre"
                android:layout_below="@+id/divider"
                android:background="#00000000"
                android:textColor="@android:color/black"
                android:layout_marginLeft="210dp" />

            <ImageButton
                android:id="@+id/btn_edit"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_below="@+id/divider"
                android:layout_marginLeft="600dp"
                android:background="#fff"
                app:srcCompat="@drawable/pencil_pink"/>

            //TIEMPO ESCUELA

            <TextView
                android:id="@+id/tvApellidoP"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/textViewNombre"
                android:layout_marginTop="20dp"
                android:textSize="20sp"
                android:text="Tiempo de escuela/trabajo:"
                android:textStyle="bold"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:textColor="@android:color/black" />

            <EditText
                android:id="@+id/textViewApellidoP"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/textViewNombre"
                android:layout_marginTop="20dp"
                android:textSize="20sp"
                android:text="Apellido Paterno"
                android:inputType="text"
                android:background="#00000000"
                android:textColor="@android:color/black"
                android:layout_toRightOf="@+id/tvApellidoP"
                android:layout_marginLeft="20dp" />

            <ImageButton
                android:id="@+id/btn_edit2"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginLeft="600dp"
                android:background="#fff"
                android:layout_below="@+id/btn_edit"
                app:srcCompat="@drawable/pencil_pink"/>

            //TIEMPO DE TRANSPORTE

            <TextView
                android:id="@+id/tvTiempoTransporte"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/tvApellidoP"
                android:layout_marginTop="20dp"
                android:textSize="20sp"
                android:text="Tiempo de transporte:"
                android:textStyle="bold"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:textColor="@android:color/black" />

            <EditText
                android:id="@+id/textViewTransporte"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/textViewApellidoP"
                android:layout_marginTop="20dp"
                android:textSize="20sp"
                android:text="Apellido Paterno"
                android:inputType="text"
                android:background="#00000000"
                android:textColor="@android:color/black"
                android:layout_marginLeft="220dp"/>

            <ImageButton
                android:id="@+id/btn_edit3"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginLeft="600dp"
                android:background="#fff"
                android:layout_below="@+id/btn_edit2"
                app:srcCompat="@drawable/pencil_pink"/>



            //TIEMPO DE RECREACION


            <TextView
                android:id="@+id/tvRecreacion"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/tvTiempoTransporte"
                android:layout_marginTop="20dp"
                android:textSize="20sp"
                android:text="Tiempo de recreación:"
                android:textStyle="bold"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:textColor="@android:color/black" />

            <EditText
                android:id="@+id/textViewRecreacion"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/textViewTransporte"
                android:layout_marginTop="20dp"
                android:textSize="20sp"
                android:text="Apellido Paterno"
                android:inputType="text"
                android:background="#00000000"
                android:textColor="@android:color/black"
                android:layout_marginLeft="220dp" />

            <ImageButton
                android:id="@+id/btn_edit8"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginLeft="600dp"
                android:background="#fff"
                android:layout_below="@+id/btn_edit3"
                app:srcCompat="@drawable/pencil_pink"/>


            //NIVEL DE ESTRES


            <TextView
                android:id="@+id/tvEstres"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/tvRecreacion"
                android:layout_marginTop="20dp"
                android:textSize="20sp"
                android:text="Nivel de estrés:"
                android:textStyle="bold"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:textColor="@android:color/black" />

            <EditText
                android:id="@+id/textViewEstres"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/textViewRecreacion"
                android:layout_marginTop="20dp"
                android:textSize="20sp"
                android:text="Apellido Paterno"
                android:inputType="text"
                android:background="#00000000"
                android:textColor="@android:color/black"
                android:layout_marginLeft="160dp"/>

            <ImageButton
                android:id="@+id/btn_edit9"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginLeft="600dp"
                android:background="#fff"
                android:layout_below="@+id/btn_edit8"
                app:srcCompat="@drawable/pencil_pink"/>




            //CONDICIONES Y SERVICIOS

